{
  "Leagues": {
    "SupportSSF": true,
    "SupportHardcore": true,
    "ListedLeagues": [
      "Ritual",
      "Ultimatum",
      "Expedition"
    ]
  },

  "Logging": {
    "Using": [ "Serilog.Sinks.BrowserConsole" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      }
    ]
  }
}
